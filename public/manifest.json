{
    "manifest_version": 3,
    "name": "FocusBoard",
    "version": "1.0.0",
    "description": "A productivity focused new tab dashboard built by Harshit Budhraja.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["storage", "search"],
    "host_permissions": ["https://zenquotes.io/*"],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
