{
    "manifest_version": 3,
    "name": "FocusBoard",
    "version": "1.0.0",
    "description": "A productivity focused new tab dashboard built by Harshit Budhraja.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["storage"],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
